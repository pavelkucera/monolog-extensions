{
	"name": "kucera/monolog-extensions",
	"description": "A set of Monolog extensions.",
	"type": "library",
	"keywords": ["Monolog", "Tracy", "log", "logging", "blue screen"],
	"homepage": "https://github.com/pavelkucera/monolog-extensions",
	"license": ["MIT", "GPL-2.0", "GPL-3.0"],
	"authors": [
		{
			"name": "Pavel Kuƒçera",
			"homepage": "https://github.com/pavelkucera"
		}
	],
	"support": {
		"issues": "https://github.com/pavelkucera/monolog-extensions/issues"
	},
	"require": {
		"php": "~7.0",
		"monolog/monolog": "~1.0",
		"tracy/tracy": "~2.3.0"
	},
	"require-dev": {
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"phing/phing": "2.15.2",
		"phpunit/phpunit": "5.6.2",
		"shipito/coding-standard": "0.6.0"
	},
	"autoload": {
		"psr-4": {
			"Kucera\\Monolog\\": "src/Monolog/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Kucera\\Monolog\\": "tests/Monolog/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0-dev"
		}
	},
	"config": {
		"bin-dir": "bin"
	}
}
